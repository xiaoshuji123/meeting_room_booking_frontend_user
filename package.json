{
  "name": "meeting_room_booking_frontend_user",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:ts": "eslint . --max-warnings 0 --ext .ts,.tsx --fix",
    "lint:css": "stylelint \"./src/**/*.{css,less}\" --fix"
  },
  "dependencies": {
    "@types/react-css-modules": "^4.6.8",
    "antd": "^5.25.1",
    "normalize.css": "^8.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0"
  },
  "devDependencies": {
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.13.5",
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-plugin-react-css-modules": "^5.2.6",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "less": "^4.3.0",
    "lint-staged": "^16.0.0",
    "postcss-less": "^6.0.0",
    "prettier": "^3.5.3",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,less}": [
      "prettier --write"
    ],
    "src/**/*.{css,less}": [
      "stylelint --fix"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  }
}
